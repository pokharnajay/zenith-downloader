{
  "version": 2,
  "functions": {
    "api/*.py": {
      "runtime": "@vercel/python@4.3.1",
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/py/analyze",
      "dest": "/api/analyze.py"
    },
    {
      "src": "/api/py/download",
      "dest": "/api/download.py"
    }
  ]
}
