{
  "version": 2,
  "functions": {
    "api/*.py": {
      "runtime": "@vercel/python@4.3.1",
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/py-analyze",
      "destination": "/api/analyze.py"
    },
    {
      "source": "/api/py-download",
      "destination": "/api/download.py"
    }
  ]
}
